{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends":[ 
    "eslint:recommended",
    "plugin:prettier/recommended"  
    ], 
    "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
